{% sw_extends '@SliderText/storefront/block/cms-block-image-slider.html.twig' %}

{% block block_image_slider %}
    {% set element = block.slots.getSlot('imagesSliderTxt') %}
    {% set columns = 1 %}

    <div class="col-12" data-cms-element-id="{{ element.id }}">
        {% block block_image_slider_inner %}
            {% sw_include "@Storefront/storefront/element/cms-element-" ~ element.type ~ ".html.twig" ignore missing %}
        {% endblock %}
    </div>
{% endblock %}
